<table class="table table-striped table-bordered table-hover table-sm">
<thead>
<tr>
<th>
<div class="row">
	<div class="col-xs-1"></div>
	<div class="col-xs-5">Name</div>
	<div class="col-xs-2">Temperature</div>
	<div class="col-xs-2">Target temperature</div>
	<div class="col-xs-2">Pid control</div>
</div>
</th>
</tr>
</thead>
<tbody>
{{#each sensors}}
<tr>
	<td>
		<heatcontrol-sensor {sensor}='.' />
	</td>
</tr>
{{/each}}
</tbody>
</table>
